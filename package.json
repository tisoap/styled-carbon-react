{
  "name": "react-ibm-digital-design",
  "version": "0.1.0",
  "private": false,
  "description": "IBM's Digital Design system as React components",
  "keywords": [
    "IBM",
    "Northstar",
    "component",
    "design",
    "digital",
    "javascript",
    "library",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:tisoap/react-ibm-digital-design.git"
  },
  "license": "MIT",
  "author": "Tiso Alvarez Puccinelli",
  "files": [
    "library",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "main": "library/index.js",
  "module": "library/es/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel source --out-dir library",
    "build:docs": "docz build",
    "build:es": "cross-env BABEL_ENV=es babel source --out-dir library/es",
    "clean": "rimraf library && mkdir library",
    "docs": "docz dev"
  },
  "dependencies": {
    "react": "16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "cross-env": "5.2.0",
    "docz": "1.0.0",
    "docz-theme-default": "1.0.0",
    "rimraf": "2.6.3"
  }
}
